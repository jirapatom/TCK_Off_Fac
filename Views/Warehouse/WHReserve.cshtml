@{
    ViewBag.Title = "";
}
@Scripts.Render("~/WarehouseRS/js")
<link rel="stylesheet" href="~/Content/css/bootstrap-select/bootstrap-select.css">
<script src="~/Content/js/bootstrap-select/bootstrap-select.js"></script>
<script src="~/Content/js/dialog/sweetalert2.min.js"></script>
<script src="~/Content/js/animation/animation-active.js"></script>
<link rel="stylesheet" href="~/Content/css/jquery.dataTables.min.css" />
<link rel="stylesheet" href="~/Content/css/animate.css" />
<link rel="stylesheet" href="~/Content/css/animation/animation-custom.css" />
<link rel="stylesheet" href="~/Content/css/dialog/sweetalert2.min.css">
<link rel="stylesheet" href="~/Content/css/dialog/dialog.css">
<body id="Body">
    <div class="row mg-t-10" style="display:none" id="FormCreateRS">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="form-element-list">
                <div class="cmp-tb-hd bcs-hd text-center">
                    <h2>เบิกครุภัณฑ์จากสถานที่จัดเก็บ</h2>
                    <p>กรอกข้อมูล จำนวน สถานที่จัดเก็บ</p>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>หมายเลขครุภัณฑ์</i>
                            <div class="input-group  nk-int-st">
                                <input id="Prefix" type="text" class="form-control" placeholder="Prefix" readonly>
                            </div>
                        </div>
                    </div>
                    <label id="ID_PL" hidden></label>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ชื่อครุภัณฑ์</i>
                            <div class="input-group  nk-int-st">
                                <input id="Item_Name" type="text" class="form-control" placeholder="Item" readonly>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>แผนก</i>
                            <div class="input-group  nk-int-st">
                                <input id="Customer" type="text" class="form-control" placeholder="Customer" readonly>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>จำนวนต้องการเบิก</i>
                            <div class="input-group  nk-int-st">
                                <input id="Plan_Qty" type="text" class="form-control" placeholder="" readonly>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>จำนวน </i><i style="color:red">**</i><i id="Show_QTY" style="color:darkcyan"></i>
                            <div class="input-group  nk-int-st">
                                <input data-type="warning" id="WHRS_Qty" type="text" class="form-control" placeholder="กรอกจำนวน" onkeyup="this.value=this.value.replace(/[^0-9,.]/g,'')">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg" id="data_2">
                            <i>วันที่เบิกเอกสาร</i>
                            <div class="input-group date nk-int-st">
                                <span class="input-group-addon"></span>
                                <input id="Date" type="text" class="form-control" readonly>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>Ref.</i>
                            <div class="input-group nk-int-st">
                                <input id="Ref" type="text" class="form-control" placeholder="กรอกเอกสารอ้างอิง" readonly />
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>สถานที่จัดเก็บที่เบิก</i><i style="color:red"> **</i>
                            <div class="input-group nk-int-st">
                                <input id="Loc_RS" type="text" class="form-control" placeholder="ยิงบาร์โค้ดสถานที่จัดเก็บ" />
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ผู้บันทึก</i><i style="color:red"> **</i>
                            <div class="input-group nk-int-st">
                                <input id="WHRS_User" type="text" class="form-control" placeholder="กรอกรหัสผู้บันทึก" />
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div id="Img" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mg-t-10 text-center">
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mg-t-10 text-center">
                        <a id="SaveWHRS" class="btn btn-success notika-btn-success">บันทึก</a>
                        <a href="@Url.Action("WHReserve", new {})" id="Clear" class="btn btn-warning notika-btn-warning">Clear</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="data-table-area mg-t-10">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="data-table-list">

                    <div class="basic-tb-hd" style="text-align:center">
                        <h2>รายการรอเบิกครุภัณฑ์</h2>
                    </div>

                    <div class="table-responsive">
                        <table id="data-table-basic1" class="table table-striped">
                            <thead>
                                <tr>
                                    <th>วันที่ต้องการเบิก</th>
                                    <th>หมายเลขครุภัณฑ์</th>
                                    <th>ชื่อครุภัณฑ์</th>
                                    <th width="20%">แผนก</th>
                                    <th>โดย</th>
                                    <th>วันที่สร้าง</th>
                                    <th>จำนวน</th>
                                    <th>รูป</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>วันที่ต้องการเบิก</th>
                                    <th>หมายเลขครุภัณฑ์</th>
                                    <th>ชื่อครุภัณฑ์</th>
                                    <th>แผนก</th>
                                    <th>โดย</th>
                                    <th>วันที่สร้าง</th>
                                    <th>จำนวน</th>
                                    <th>รูป</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
