@{
    ViewBag.Title = "";
}
@Scripts.Render("~/WarehouseM/js")
<link rel="stylesheet" href="~/Content/css/bootstrap-select/bootstrap-select.css">
<script src="~/Content/js/bootstrap-select/bootstrap-select.js"></script>
<script src="~/Content/js/dialog/sweetalert2.min.js"></script>
<script src="~/Content/js/animation/animation-active.js"></script>
<link rel="stylesheet" href="~/Content/css/jquery.dataTables.min.css" />
<link rel="stylesheet" href="~/Content/css/animate.css" />
<link rel="stylesheet" href="~/Content/css/animation/animation-custom.css" />
<link rel="stylesheet" href="~/Content/css/dialog/sweetalert2.min.css">
<link rel="stylesheet" href="~/Content/css/dialog/dialog.css">
<body id="Body">
    <div class="row mg-t-10" style="">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="form-element-list">
                <div class="cmp-tb-hd bcs-hd text-center">
                    <h2>ย้ายครุภัณฑ์</h2> <i style="color:red">(ย้ายครุภัณฑ์ระหว่างสถานที่จัดเก็บ)</i>
                    <p> </p>
                </div>
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">

                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ยิงบาร์โค้ดครุภัณฑ์</i><i style="color:red"> **</i>
                            <div class="input-group  nk-int-st">
                                <input id="BarCode" type="text" class="form-control" placeholder="ยิงบาร์โค้ดครุภัณฑ์">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ยิงบาร์โค้ดสถานที่จัดเก็บ</i><i style="color:red"> **</i><i id="Data_Location" style="color:red"></i>
                            <div class="input-group nk-int-st">
                                <input id="Location" type="text" class="form-control" placeholder="ยิงบาร์โค้ดสถานที่จัดเก็บ" />
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">

                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-3 col-md3 col-sm-3 col-xs-12">

                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>จำนวน (ครุภัณฑ์)</i><i style="color:red">ชิ้น</i>
                            <div class="input-group nk-int-st">
                                <input id="Qty" type="text" class="form-control" placeholder="กรอกข้อมูลครุภัณฑ์ (ชิ้น)" onkeyup="this.value=this.value.replace(/[^0-9,.]/g,'')" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 notification-demo">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ผู้บันทึก</i><i style="color:red"> **</i>
                            <div class="input-group nk-int-st">
                                 <input id="User" type="text" class="form-control" placeholder="กรอกรหัสผู้บันทึก" />
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">

                    </div>
                </div>

            

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mg-t-10 text-center">
                        <a id="SaveMove" class="btn btn-success notika-btn-success">บันทึก</a>
                        <a href="@Url.Action("MoveLocation", new {})" id="Clear" class="btn btn-warning notika-btn-warning">Clear</a>
                    </div>
                </div>

            </div>
        </div>
    </div>


    <div class="row mg-t-10" id="Detail_Item_Show" style="display:none">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="form-element-list">
                <div class="row">
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
                        <div class="cmp-tb-hd bcs-hd text-center">
                            <label class="form-body">หมายเลขเอกสาร : </label> <i id="Detail_Prefix" style="color:blue"></i>

                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
                        <div class="cmp-tb-hd bcs-hd text-center">
                            <label class="form-body">ชื่อเอกสาร : </label> <i id="Detail_ItemName" style="color:blue"></i>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="cmp-tb-hd bcs-hd text-center">
                            <label class="form-body">รายละเอียดเอกสาร : </label> <i id="Detail_ItemDes" style="color:blue"></i>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                        <div class="cmp-tb-hd bcs-hd text-center">
                            <label class="form-body">แผนกเจ้าของเอกสาร : </label> <i id="Detail_Customer" style="color:blue"></i>
                        </div>
                    </div>
                    <div class="row">
                        <div id="Img" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mg-t-10 text-center">
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>

</body>
